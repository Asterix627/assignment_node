generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

model User {
    id String @id @default(uuid()) @db.Char(36)
    firstName String 
    lastName String
    email String @unique
    password String
    role String @db.Char(36)
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now())
    token String? @db.VarChar(255)
}

